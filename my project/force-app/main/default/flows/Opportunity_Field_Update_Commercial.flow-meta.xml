                </rightValue>
            </conditions>
            <connector>
                <targetReference>Premium_Marketplace</targetReference>
            </connector>
            <label>Marketplace/Permium</label>
        </rules>
        <rules>
            <name>Premium</name>
            <conditionLogic>1 AND( 2 OR 5 OR 6) AND (3 AND 4)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.RecordTypeName__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Vendor_Package_3</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.StageName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Signed/Onboarding</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Account.Commercial_Package__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Marketplace</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.Account.Commercial_Package__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Premium &amp; Marketplace</stringValue>
                </rightValue>
